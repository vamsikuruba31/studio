
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow read and write access to any file for any authenticated user.
    // This is a permissive rule for debugging and should be tightened
    // in production for security.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
